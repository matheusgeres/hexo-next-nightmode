  background: rgb(29, 29, 29);
}

.darkScheme .header-inner{
  background: rgb(26, 26, 26);
}

.darkScheme .sidebar-inner {
	background: rgb(26, 26, 26);
  color: #75f079;
}

.darkScheme .site-author-name{
  color: #75f079;
}

.darkScheme .links-of-author-item a:hover{
  background: #222;
}

.darkScheme .sidebar a:hover{
  color: #fff;
}

.darkScheme post-expand {
	background: rgb(26, 26, 26);
}

.darkScheme .site-title {
	//background: #75f079;
	color: #75f079;
}

