	background: #75f079;
  p {
    color: black;
  }
}

.darkScheme .post-block {
	color: #75f079;
  background: rgb(26, 26, 26);
}

.darkScheme .post-title-link {
	color: #75f079;
}

.darkScheme .copy-btn {
	background: rgb(26, 26, 26);
  color: #75f079;
}

.darkScheme .btn {
	background: rgb(26, 26, 26);
  color: #75f079;
}

.darkScheme .btn:hover {
	background: #222;
  color: #75f079;
}

.darkScheme .post-copyright {
	background-color: rgb(26, 26, 26);
}

.darkScheme .menu .menu-item a {
	color: #75f079;
}

.darkScheme .menu-item-active a, .darkScheme .menu .menu-item a:hover, .darkScheme .menu .menu-item span.exturl:hover {
